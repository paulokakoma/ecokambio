#!/usr/bin/expect -f

set timeout 20
set password "1234"
set server "212.90.120.135"

spawn ssh -o StrictHostKeyChecking=no root@$server
expect "password:"
send "$password\r"
expect "#"

puts "\n--- TEST 1: HTTPS Root / ---"
send "curl -v -k https://localhost/ -H 'Host: admin.ecokambio.com'\r"
expect "#"

puts "\n--- TEST 2: HTTPS /admin ---"
send "curl -v -k https://localhost/admin -H 'Host: admin.ecokambio.com'\r"
expect "#"

interact
